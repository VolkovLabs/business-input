{
  "author": "Volkov Labs",
  "bugs": "https://github.com/marcusolsson/grafana-static-datasource/issues",
  "description": "A data source plugin for Grafana for static data",
  "devDependencies": {
    "@emotion/core": "10.0.27",
    "@grafana/data": "9.0.1",
    "@grafana/runtime": "9.0.1",
    "@grafana/toolkit": "9.0.1",
    "@grafana/ui": "9.0.1",
    "@types/lodash": "^4.14.168",
    "emotion": "10.0.27"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "Apache-2.0",
  "name": "marcusolsson-static-datasource",
  "repository": "github:volkovlabs/volkovlabs-static-datasource",
  "scripts": {
    "build": "grafana-toolkit plugin:build --coverage",
    "dev": "grafana-toolkit plugin:dev",
    "format": "prettier --write \"**\"",
    "sign": "grafana-toolkit plugin:sign",
    "start": "docker-compose pull && docker-compose up",
    "stop": "docker-compose down",
    "test": "grafana-toolkit plugin:test",
    "upgrade": "yarn upgrade --latest",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "version": "2.0.0"
}
