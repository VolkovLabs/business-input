    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^11.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.77.8",
    "sass-loader": "^15.0.0",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
